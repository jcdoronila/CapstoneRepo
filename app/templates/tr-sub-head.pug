  style.
    #navbar {
      overflow: hidden;
      background-color: rgb(44, 48, 59);
      z-index: 10;
      box-shadow: 0 2px 50px -2px rgba(0, 0, 0, .2);
    }

    #navbar span {
      margin-left: 0;
      margin-right: 10px;
    }

    #navbar-hidden {
      position: relative;
      padding: 16px 0;
      background-color: gold;
      color: rgb(87, 83, 83);
      box-shadow: inset 0px -38px 40 px -27px rgba(0, 0, 0, 0.15);
    }

    #navbar-hidden a {
      text-decoration: none;
      color: rgb(61, 59, 59);
      padding: 14px 14px;
    }

    #navbar a {
      float: left;
      display: block;
      color: #f2f2f2;
      padding: 24px;
      text-decoration: none;
      font-size: 15.4px;
    }

    #navbar a:hover {
      background-color: rgba(76, 79, 87, 0.37);
    }

    #navbar-hidden img {
      width: 3em;
      padding: 0;
      cursor: pointer;
    }

    .sticky {
      position: fixed;
      top: 0;
      width: 100%;
    }

    .sticky+.container {
      padding-top: 70px;
    }

    .content {
      padding: 16px;
    }

    .dropdown img {
      margin-left: 10px;
    }

    .dropdown span {
      margin-right: 10px;
      margin-left: 10px;
    }

    .dropdown-menu.extended.tasks-bar {
      min-width: 140px !important;
      max-width: 140px !important;
      width: 140px !important;
    }

    .dropdown-menu.extended.inbox {
      top: 25px;
    }

    #return-to-top {
      position: fixed;
      bottom: 20px;
      right: 20px;
      background-color: rgb(44, 48, 59);
      width: 50px;
      height: 50px;
      display: block;
      text-decoration: none;
      border-radius: 35px;
      display: none;
      transition: all 2 ease;
      z-index:1000;
    }

    #return-to-top i {
      color: #fff;
      margin: 0;
      position: relative;
      left: 15px;
      top: 5px;
      font-size: 24pt;
      font-weight: bold;
      transition: all 0.3s ease;
     
    }

    #return-to-top:hover {
      background-color: rgb(25, 27, 34);
     
    }

    #return-to-top:hover i {
      color: #fff;
      top: -5px;
    }

    #header_inbox_bar i {
      font-size: 20px;
      font-weight: 600;
      color:black;
    }

    .badge.bg-theme {
      background-color: #FF4961;
      box-shadow: 0px 0px 10px #FF4961;
      position: absolute;
      top: -13px;
      left: 14px;
    }

    .photo img {
      width: 50px !important;
      height: 50px !important;
      border-radius: 50% !important;
    }

    .content-panel {
      padding: 30px;
      border-radius: 0.35rem;
      border: none;
      box-shadow: 0px 1px 15px 1px rgba(62, 57, 107, 0.07);
    }

    #manage h4 {
      margin-left: 0;
    }

    @media screen and (max-width: 500px) {
      #navbar-hidden .sm {
        display: none !important;
      }
    }

    footer.site-footer {
    position: relative;
    right: 0;
    bottom: 0;
    left: 0;
    padding: 1rem;
    text-align: center;
    }

block main